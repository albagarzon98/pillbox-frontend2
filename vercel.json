{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/PillBox-FrontEnd"
      }
    }
  ],
  "routes": [
    { 
      "src": "/assets/(.*)", 
      "dest": "/assets/$1" 
    },
    {
      "src": "/(?!.*\\.(js|css|png|jpg|jpeg|gif|ico|json|map)$).*",
      "dest": "/index.html"
    }
  ]
}
